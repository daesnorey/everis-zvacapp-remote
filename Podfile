# platform :ios, '9.0'

target 'Prueba' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Prueba
# add the Firebase pod for Google Analytics
pod 'Firebase/Analytics'
# add pods for any other desired Firebase products
